{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": ["src/*", "*"],
      "react-native-launch-navigator": ["src/types/react-native-launch-navigator"]
    },
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "lib": ["ES6"],
    "module": "ESNext",
    "moduleResolution": "node",
    "typeRoots": ["node_modules/@types", "src/types"],
    "noEmit": true,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "resolveJsonModule": true,
    "target": "esnext",
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "src/types",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js",
    "react-native.config.js",
    "rn-cli.config.js",
    "__mocks__/**",
    "coverage/**"
  ]
}
