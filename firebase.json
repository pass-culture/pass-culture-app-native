{
  "functions": {
    "source": "server"
  },
  "hosting": [
    {
      "target": "testing",
      "public": "dist",
      "ignore": [
        "firebase.json",
        "babel.config.js",
        "coverage",
        "jest",
        "jest.config.ts",
        "sonar-project.properties",
        "src",
        "tests",
        "tsconfig.json",
        "tsconfig.tsbuildinfo",
        "yarn.lock",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "function": "serverTesting"
        }
      ]
    }
  ]
}
